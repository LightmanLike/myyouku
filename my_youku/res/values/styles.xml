<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="TextTitleStyle">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/dark_gray</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:padding">0dip</item>
    </style>

    <style name="TextTitleDivider">
        <item name="android:layout_width">1dip</item>
        <item name="android:layout_height">22dip</item>
        <item name="android:src">#e5e5e5</item>
    </style>

    <style name="MediaController_SeekBar" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:indeterminateDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/scrubber_control_selector_holo</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>
    
    <style name="Volumn_SeekBar" parent="android:Widget.SeekBar">
        <item name="android:paddingLeft">13dip</item>
        <item name="android:paddingRight">13dip</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:thumb">@drawable/play_icon_volume_ball</item>
        <item name="android:progressDrawable">@drawable/volumn_progress</item>
    </style>    

</resources>